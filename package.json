{
  "name": "todo-list",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "aspida --watch",
    "build": "aspida && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "cover": "jest --coverage",
    "cover:update": "jest -u",
    "test": "yarn lint && yarn typecheck && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -x eslint -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "prepare": "if [ ! -f .env ]; then cp .env.example .env; fi && husky install && aspida",
    "sb": "start-storybook -p 6006 -s ./public",
    "build-sb": "build-storybook -s public",
    "analyze": "aspida && ANALYZE=true next build"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
    "@next/bundle-analyzer": "^12.0.4",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "next": "^12.0.4",
    "node-cache": "^5.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-paginate": "^7.1.5",
    "reflect-metadata": "^0.1.13",
    "swr": "^1.0.1",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-storyshots": "^6.3.12",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-paginate": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "dotenv": "^10.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "storybook-addon-performance": "^0.16.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.4.4",
    "webpack": "^5.64.1"
  },
  "engines": {
    "node": ">=14",
    "npm": "use_yarn_instead",
    "yarn": "^1"
  }
}

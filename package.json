{
  "name": "todo-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "aspida --watch",
    "build": "aspida && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "cover": "jest --coverage",
    "cover:update": "jest -u",
    "test": "yarn lint && yarn typecheck && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "prepare": "if [ ! -f .env ]; then cp .env.example .env; fi && husky install && aspida",
    "sb": "start-storybook -p 6006 -s ./public",
    "build-sb": "build-storybook -s public",
    "analyze": "aspida && ANALYZE=true next build",
    "migrate": "ts-node -r tsconfig-paths/register src/bin/migrate.ts"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
    "@auth0/auth0-react": "^1.8.0",
    "@next/bundle-analyzer": "^12.0.7",
    "@notionhq/client": "^0.4.9",
    "@slack/webhook": "^6.0.0",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "jsonwebtoken": "^8.5.1",
    "next": "^12.0.7",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.21.2",
    "react-icons": "^4.3.1",
    "react-native-web": "^0.17.5",
    "react-paginate": "^8.1.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "swr": "^1.1.0",
    "tsyringe": "^4.6.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@peculiar/webcrypto": "^1.2.3",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-storyshots": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@technote-space/aspida": "^1.7.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-paginate": "^7.1.1",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "babel-jest": "^27.4.4",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "dotenv": "^10.0.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-storybook": "^0.5.3",
    "eslint-plugin-strict-dependencies": "^0.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.4",
    "lint-staged": "^12.1.2",
    "msw": "^0.36.3",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^12.4.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0"
  },
  "engines": {
    "node": ">=14",
    "npm": "use_yarn_instead",
    "yarn": "^1"
  }
}

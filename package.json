    "@prisma/client": "^3.7.0",
    "@slack/webhook": "^6.0.0",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "jsonwebtoken": "^8.5.1",
    "native-base": "^3.3.0",
    "next": "^12.0.7",
    "node-cache": "^5.1.2",
    "react": "17.0.2",
    "react-datepicker": "^4.6.0",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.22.5",
    "react-icons": "^4.3.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "^0.17.5",
    "react-paginate": "^8.1.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "swr": "^1.1.2",
    "tsyringe": "^4.6.0",
    "validator": "^13.7.0",
    "yup": "^0.32.11",
    "yup-locale-ja": "^0.1.3"
  },

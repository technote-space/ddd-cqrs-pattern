{
  "name": "todo-list",
  "version": "0.5.0",
  "private": true,
  "description": "Todo List with DDD+CQRS",
  "keywords": [
    "ddd",
    "cqrs",
    "notion"
  ],
  "homepage": "https://github.com/technote-space/ddd-cqrs-pattern",
  "bugs": {
    "url": "https://github.com/technote-space/ddd-cqrs-pattern/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/ddd-cqrs-pattern.git"
  },
  "license": "MIT",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "aspida --watch",
    "build": "aspida && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "cover": "jest --coverage",
    "cover:update": "jest -u",
    "test": "yarn lint && yarn typecheck && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "prepare": "if [ ! -f .env ]; then cp .env.example .env; fi && if [ ! -f infra/.env ]; then cp infra/.env.example infra/.env; fi  && husky install && aspida && prisma generate --schema ./schema/schema.prisma",
    "sb": "start-storybook -p 6006 -s ./public",
    "build-sb": "build-storybook -s public",
    "analyze": "aspida && ANALYZE=true next build",
    "migrate": "ts-node -r tsconfig-paths/register src/bin/migrate.ts",
    "mysql:migrate": "prisma migrate dev --schema ./schema/schema.prisma --name init",
    "mysql:up": "docker-compose -f infra/docker-compose.yml --env-file infra/.env up -d",
    "mysql:down": "docker-compose -f infra/docker-compose.yml down --remove-orphans"
  },
  "resolutions": {
    "resolve": "1.20.0"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
